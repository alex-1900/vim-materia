
{
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.snippets.enable": true,
    "coc.source.around.firstMatch": false,
    "coc.source.buffer.firstMatch": false,
    "coc.preferences.rootPatterns": [ "composer.json", "wp-content", ".git" ],
    "coc.source.buffer.enable": true,
    "coc.source.around.enable": false,
    "coc.source.tag.enable": false,
    "coc.preferences.colorSupport": true,
    "intelephense.licenceKey": "00A06Y6SRSREAD4",
    "eslint.packageManager": "yarn",
    "eslint.nodePath": ".yarn/sdks",
    "tsserver.tsdk": ".yarn/sdks/typescript/lib",
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "completeUnimported": true,
                "usePlaceholders": true
            }
        }
    }
}
